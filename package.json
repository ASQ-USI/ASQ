{
  "name": "asq",
  "version": "0.3.0-alpha",
  "description": "Synchronous slideshow mixed with a classroom clicker.",
  "author": "Jacques DAFFLON <jacques.dafflon@gmail.com>, Margarita GRINVALD <grinvalm@student.ethz.ch>, Max VON BUELOW, Vasileios Triglianos <vasileios.triglianos@usi.ch>",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ASQ-USI/ASQ.git"
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "test": "make test-unit",
    "start-dev": "NODE_ENV=development nodemon | bunyan",
    "start": "NODE_ENV=production node --harmony app.js",
    "install": "bower --allow-root install && npm run presenterControl && npm run plugins",
    "webpack-dev": "NODE_ENV=development webpack -d --watch",
    "webpack-prod": "NODE_ENV=production webpack -p",
    "js-vendor-dev": "NODE_ENV=development tasks/vendor",
    "js-vendor-prod": "NODE_ENV=production tasks/vendor",
    "elements": "mkdir -p public/elements && cp client/elements/* public/elements",
    "fonts": "mkdir -p public/fonts && cp client/fonts/* public/fonts",
    "img": "mkdir -p public/img && cp client/img/* public/img",
    "css": "mkdir -p public/css && cp client/css/*.css public/css",
    "favicons": "cp client/favicons/* public",
    "static": "npm run elements && npm run fonts && npm run img && npm run css && npm run favicons",
    "less-dev": "autoless client/less public/css",
    "less-prod": "autoless --no-watch client/less public/css",
    "dust-dev": "dustc --watch views/shared/**/*.dust -o client/js/templates.js",
    "dust-prod": "dustc views/shared/**/*.dust -o client/js/templates.js",
    "clean": "rimraf slides/* test/slides/*",
    "plugins": "tasks/plugins",
    "presenterControl": "tasks/presenterControl",
    "db": "tasks/db",
    "watch": "parallelshell 'npm run webpack-dev' 'npm run dust-dev' 'npm run less-dev'",
    "build": "npm run webpack-prod && npm run js-vendor-dev && npm run static && npm run less-prod && npm run dust-prod",
    "dev": "NODE_ENV=development npm run js-vendor-dev && npm run static && parallelshell 'npm run watch' 'npm run start-dev'"
  },
  "dependencies": {
    "asq-exercise": "ASQ-USI-Elements/asq-exercise#feat-polymer-1",
    "asq-microformat": "^0.3.8",
    "asq-plugin": "ASQ-USI/asq-plugin",
    "asq-settings": "ASQ-USI-Elements/asq-settings#feat-polymer-1",
    "asq-visualization": "0.0.1",
    "async": "~0.2.9",
    "bcrypt": "^0.8.2",
    "bluebird": "^2.7.1",
    "body-parser": "^1.12.2",
    "bows": "^1.1.2",
    "cheerio": "^0.19.0",
    "connect": "2.8.x",
    "connect-flash": "~0.1.1",
    "connect-redis": "^2.2.0",
    "consolidate": "~0.10.0",
    "cookie": "~0.1.0",
    "cookie-parser": "^1.3.4",
    "dustjs-helpers": "^1.7.1",
    "dustjs-linkedin": "^2.7.1",
    "eventemitter2": "^0.4.14",
    "express": "^4.13.3",
    "express-session": "^1.10.4",
    "grunt-webpack": "^1.0.8",
    "hammerjs": "^2.0.4",
    "imports-loader": "^0.6.3",
    "impress-asq-adapter": "ASQ-USI/impress-asq-adapter",
    "impress-asq-fork-asq-adapter": "ASQ-USI/impress-asq-fork-asq-adapter#master",
    "jquery": "^2.1.0",
    "jsonwebtoken": "^5.0.1",
    "jws": "^3.0.0",
    "ldapauth-fork": "^2.2.12",
    "livereload": "^0.3.7",
    "lodash": "^2.4.1",
    "logger-asq": "ASQ-USI/logger-asq#master",
    "mkdirp": "~0.3.5",
    "moment": "^2.8.3",
    "mongoose": "^4.0.1",
    "mongoose-schema-extend": "^0.1.7",
    "morgan": "^1.5.2",
    "multer": "^0.1.8",
    "node-libs-browser": "^0.5.2",
    "node-redis": "^0.1.7",
    "passport": "^0.2.0",
    "passport-local": "~0.1.6",
    "passport.socketio": "~0.0.5",
    "promised-io": "~0.3.3",
    "redis": "~0.8.4",
    "rimraf": "^2.3.4",
    "serve-favicon": "^2.2.0",
    "serve-static": "^1.9.2",
    "session-mongoose": "~0.2.2",
    "sillyname": "0.0.3",
    "simple-errors": "0.0.4",
    "socket.io": "^1.3.5",
    "socket.io-browserify": "~0.9.6",
    "socket.io-client": "^1.3.5",
    "socket.io-redis": "^0.1.3",
    "socketio-jwt": "^2.3.5",
    "stylus": "~0.32.1",
    "sugar": "~1.3.8",
    "superagent": "~0.15.7",
    "unzip": "~0.1.7",
    "validator": "^3.5.1",
    "when": "^2.8.0"
  },
  "engines": {
    "node": "^0.12.0",
    "npm": "^2.0.0"
  },
  "devDependencies": {
    "autoless": "^0.1.7",
    "bower": "^1.4.1",
    "browserify": "~2.35.1",
    "bunyan": "^1.4.0",
    "chai": "^2.2.0",
    "chai-as-promised": "^5.0.0",
    "css-loader": "^0.14.4",
    "dust-loader": "^1.0.1",
    "exports-loader": "^0.6.2",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "mocha": "^1.18.2",
    "nodemon": "^1.3.7",
    "parallelshell": "^1.1.1",
    "proxyquire": "~0.5.0",
    "sandboxed-module": "^2.0.0",
    "sinon": "^1.9.0",
    "sinon-as-promised": "^3.0.0",
    "sinon-chai": "^2.5.0",
    "style-loader": "^0.12.3",
    "supertest": "~0.8.0",
    "uglify-js": "^2.3.6",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "asq": {
    "corePlugins": [
      "asq-settings",
      "asq-exercise"
    ]
  },
  "keywords": [
    "node.js"
  ],
  "license": "MIT",
  "readmeFilename": "README.md"
}
