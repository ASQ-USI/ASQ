{
  "name": "asq",
  "version": "0.3.0-alpha",
  "description": "Synchronous slideshow mixed with a classroom clicker.",
  "author": "Jacques DAFFLON <jacques.dafflon@gmail.com>, Margarita GRINVALD <grinvalm@student.ethz.ch>, Max VON BUELOW, Vasileios Triglianos <vasileios.triglianos@usi.ch>",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ASQ-USI/ASQ.git"
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "test": "NODE_ENV=test mocha --reporter spec --bail --check-leaks test/unit/*",
    "start-dev": "NODE_ENV=development nodemon app.js dotenv_config_silent=true| bunyan",
    "start": "NODE_ENV=production node -r dotenv/config app.js dotenv_config_silent=true",
    "install-private-dep": "CI_USER_TOKEN=${CI_USER_TOKEN} npm run install-private-dependencies",
    "install-external-dep": "bower --allow-root cache clean && bower --allow-root install && CI_USER_TOKEN=${CI_USER_TOKEN} npm run ui && npm run cockpit && npm run plugins && npm run s2h-install && npm run pdf-bower-components",
    "webpack:dev": "NODE_ENV=development webpack -d --watch",
    "webpack:prod": "NODE_ENV=production webpack -p",
    "js-vendor-dev": "NODE_ENV=development tasks/vendor",
    "js-vendor-prod": "NODE_ENV=production tasks/vendor",
    "fonts": "mkdir -p public/fonts && cp client/fonts/* public/fonts",
    "img": "mkdir -p public/img && cp client/img/* public/img",
    "css": "mkdir -p public/css && cp client/css/*.css public/css",
    "favicons": "cp client/favicons/* public",
    "static": "npm run fonts && npm run img && npm run css && npm run favicons",
    "less-dev": "autoless client/less public/css",
    "less-prod": "autoless --no-watch client/less public/css",
    "dust-dev": "dustc --watch views/shared/**/*.dust -o client/js/templates.js",
    "dust-prod": "dustc views/shared/**/*.dust -o client/js/templates.js",
    "clean": "rimraf slides/* test/slides/*",
    "plugins": "tasks/plugins",
    "cockpit": "tasks/cockpit",
    "ui": "tasks/ui",
    "s2h-install": "tasks/slide2html",
    "install-private-dependencies": "tasks/install-private-dependencies",
    "pdf-bower-components" : "tasks/pdf-bower-components",
    "live-app-bower-components" : "tasks/live-app-bower-components",
    "db": "tasks/db",
    "watch": "parallelshell 'npm run webpack:dev' 'npm run dust-dev' 'npm run less-dev'",
    "build": "npm run webpack:prod && npm run js-vendor-prod && npm run static && npm run less-prod && npm run dust-prod",
    "dev": "NODE_ENV=development npm run js-vendor-dev && npm run static && parallelshell 'npm run watch' 'npm run start-dev'",
    "//": "DevOps commands",
    "w-start-dev": "export X_NPM_RUN_COMMAND='npm run start-dev' && wercker --environment=wercker.env dev",
    "w-local-start-dev": "npm run w-start-dev -- --docker-local"
  },
  "dependencies": {
    "archiver": "^1.1.0",
    "asq-buckets-q": "ASQ-USI-Elements/asq-buckets-q#master",
    "asq-canvas": "ASQ-USI-Elements/asq-canvas#master",
    "asq-code-q": "ASQ-USI-Elements/asq-code-q#master",
    "asq-css-select-q": "ASQ-USI-Elements/asq-css-select-q#master",
    "asq-exercise": "ASQ-USI-Elements/asq-exercise#master",
    "asq-fiddle-q": "ASQ-USI-Elements/asq-fiddle-q#master",
