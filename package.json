{
  "name": "asq",
  "version": "0.3.0-alpha",
  "description": "Synchronous slideshow mixed with a classroom clicker.",
  "author": "Jacques DAFFLON <jacques.dafflon@gmail.com>, Margarita GRINVALD <grinvalm@student.ethz.ch>, Max VON BUELOW, Vasileios Triglianos <vasileios.triglianos@usi.ch>",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ASQ-USI/ASQ.git"
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "config": {
    "docker": {
      "tag": "",
      "devuser": "asqhub",
      "devrepo": "asq-dev"
    }
  },
  "scripts": {
    "test": "NODE_ENV=test mocha --reporter spec --bail --check-leaks test/unit/*",
    "start-dev": "NODE_ENV=development nodemon app.js dotenv_config_silent=true| bunyan",
    "start": "NODE_ENV=production node -r dotenv/config app.js dotenv_config_silent=true",
    "install-private-dep": "CI_USER_TOKEN=${CI_USER_TOKEN} npm run install-private-dependencies",
    "install-external-dep": "bower --allow-root cache clean && bower --allow-root install && CI_USER_TOKEN=${CI_USER_TOKEN} npm run install-private-dependencies && npm run plugins && npm run pdf-bower-components && npm run live-app-bower-components",
    "webpack:dev": "NODE_ENV=development webpack -d --watch",
    "webpack:dev-no-watch": "NODE_ENV=development webpack -d",
    "webpack:prod": "NODE_ENV=production webpack -p",
    "js-vendor:dev": "NODE_ENV=development tasks/vendor",
    "js-vendor:prod": "NODE_ENV=production tasks/vendor",
    "fonts": "mkdir -p public/fonts && cp client/fonts/* public/fonts",
    "img": "mkdir -p public/img && cp client/img/* public/img",
    "css": "mkdir -p public/css && cp client/css/*.css public/css",
    "favicons": "cp client/favicons/* public",
    "static": "npm run fonts && npm run img && npm run css && npm run favicons",
    "less-dev": "autoless client/less public/css",
    "less-prod": "autoless --no-watch client/less public/css",
    "dust:dev": "dustc --watch views/shared/**/*.dust -o client/js/templates.js",
    "dust:prod": "dustc views/shared/**/*.dust -o client/js/templates.js",
    "clean": "rimraf slides/* test/slides/*",
    "plugins": "tasks/plugins",
    "cockpit": "tasks/cockpit",
    "ui": "tasks/ui",
    "s2h-install": "tasks/slide2html",
    "install-private-dependencies": "tasks/install-private-dependencies",
    "pdf-bower-components": "tasks/pdf-bower-components",
    "live-app-bower-components": "tasks/live-app-bower-components",
    "db": "tasks/db",
    "watch": "parallelshell 'npm run webpack:dev' 'npm run dust:dev' 'npm run less-dev'",
    "build:pro": "npm run webpack:prod && npm run js-vendor:prod && npm run static && npm run less-prod && npm run dust:prod",
    "build:dev": "npm run webpack:dev-no-watch && npm run js-vendor:dev && npm run static && npm run less-prod && npm run dust:prod",
    "dev": "NODE_ENV=development npm run js-vendor:dev && npm run static && parallelshell 'npm run watch' 'npm run start-dev'",
    "//": "wercker --verbose --environment=devops.env pull <build_id> --branch <branch_name> --load",
    "w-start-dev": "export X_NPM_RUN_COMMAND='npm run start-dev' && wercker --environment=devops.env dev --expose-ports",
    "w-start": "export X_NPM_RUN_COMMAND='npm run start' && wercker --environment=devops.env dev --expose-ports",
    "w-dev": "export X_NPM_RUN_COMMAND='npm run dev' && wercker --environment=devops.env dev --expose-ports",
    "w-dev-tests": "wercker --environment=devops.env dev --pipeline=dev-tests",
    "w-build": "export X_CI='false' && wercker --environment=devops.env build",
    "w-local-start-dev": "npm run w-start-dev -- --docker-local",
    "w-local-start": "npm run w-start -- --docker-local",
    "w-local-dev": "npm run w-dev -- --docker-local",
    "w-local-dev-tests": "npm run w-dev-tests -- --docker-local",
    "w-local-v-start-dev": "npm run w-start-dev -- --docker-local --verbose",
    "w-local-build": "npm run w-build -- --docker-local",
    "dc-asq-up": "DOCKER_HUB_REPO=$npm_package_config_docker_devuser/$npm_package_config_docker_devrepo DOCKER_HUB_IMAGE_TAG=$npm_package_config_docker_tag COMPOSE_HTTP_TIMEOUT=120 docker-compose up; docker-compose stop; docker-compose rm -f -v; docker volume rm asq_ASQSlide asq_ASQNginx asq_ASQRedisData asq_ASQMongoData",
    "d-rm-containers": "echo 'Removed:' && docker rm -f -v $(docker ps -a -q)",
    "d-rm-images-none": "echo 'Removed:' && docker rmi $(docker images | grep '<none>' | awk '{print $3}')",
    "d-rm-images-all": "echo 'Removed:' && docker rmi $(docker images | grep -v 'IMAGE' | awk '{print $3}')",
    "docker-delete-registry-image": "DOCKER_HUB_DEV_REPO=$npm_package_config_docker_devrepo node lib/support/docker/hub/delete-tags.js"
  },
  "dependencies": {
    "archiver": "^1.1.0",
    "asq-buckets-q": "ASQ-USI-Elements/asq-buckets-q#master",
    "asq-canvas": "ASQ-USI-Elements/asq-canvas#master",
    "asq-code-q": "ASQ-USI-Elements/asq-code-q#master",
    "asq-css-select-q": "ASQ-USI-Elements/asq-css-select-q#master",
    "asq-exercise": "ASQ-USI-Elements/asq-exercise#master",
    "asq-fiddle-q": "ASQ-USI-Elements/asq-fiddle-q#master",
    "asq-highlight-q": "ASQ-USI-Elements/asq-highlight-q#master",
    "asq-js-eval-q": "ASQ-USI-Elements/asq-js-eval-q#master",
    "asq-js-function-body-q": "ASQ-USI-Elements/asq-js-function-body-q#master",
    "asq-multi-choice-q": "ASQ-USI-Elements/asq-multi-choice-q#master",
    "asq-order-q": "ASQ-USI-Elements/asq-order-q#master",
    "asq-plugin": "ASQ-USI/asq-plugin",
    "asq-rating-q": "ASQ-USI-Elements/asq-rating-q#master",
    "asq-settings": "ASQ-USI-Elements/asq-settings#master",
    "asq-sqlite-q": "ASQ-USI-Elements/asq-sqlite-q#master",
    "asq-text-input-q": "ASQ-USI-Elements/asq-text-input-q#master",
    "asq-live-app": "https://github.com/ASQ-USI/live-app-asq.git#feat-student-question-events",
    "asq-visualization": "0.0.1",
    "async": "~0.2.9",
    "bcrypt": "^0.8.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.12.2",
    "boom": "^4.1.0",
    "bows": "^1.1.2",
    "cheerio": "^0.20.0",
    "connect": "2.8.x",
    "connect-flash": "~0.1.1",
    "connect-redis": "^2.2.0",
    "consolidate": "~0.10.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "~2.0.0",
    "dropzone": "^4.3.0",
    "dustjs-helpers": "^1.7.1",
    "dustjs-linkedin": "^2.7.1",
    "eventemitter2": "^0.4.14",
    "express": "^4.13.3",
    "express-session": "^1.10.4",
    "file-type": "^3.3.0",
    "filenamify": "^1.2.1",
    "fs-extra": "^0.30.0",
    "grunt-webpack": "^1.0.8",
    "hammerjs": "^2.0.4",
    "helmet": "^2.1.1",
    "imports-loader": "^0.6.3",
    "impress-asq-adapter": "ASQ-USI/impress-asq-adapter",
    "impress-asq-fork-asq-adapter": "ASQ-USI/impress-asq-fork-asq-adapter#master",
    "jquery": "^2.1.0",
    "jws": "^3.0.0",
    "ldapauth-fork": "^2.2.12",
    "livereload": "^0.3.7",
    "lodash": "^4.15.0",
    "logger-asq": "ASQ-USI/logger-asq#master",
    "mocha": "^3.2.0",
    "moment": "^2.8.3",
    "mongo-sanitize": "^1.0.0",
    "mongodb-migrate": "^1.2.3",
    "mongoose": "^4.7.0",
    "mongoose-schema-extend": "^0.1.7",
    "morgan": "^1.5.2",
    "multer": "^0.1.8",
    "node-libs-browser": "^0.5.2",
    "node-redis": "^0.1.7",
    "nodemon": "^1.9.2",
    "passport": "^0.3.2",
    "passport-local": "~0.1.6",
    "passport.socketio": "^3.6.2",
    "promised-io": "~0.3.3",
    "read-chunk": "^1.0.1",
    "redis": "^2.6.3",
    "reveal-asq-fork-asq-adapter": "github:youyix/reveal-asq-fork-asq-adapter",
    "serve-favicon": "^2.2.0",
    "serve-static": "^1.9.2",
    "session-mongoose": "~0.2.2",
    "sillyname": "0.0.3",
    "simple-errors": "0.0.4",
    "socket.io": "^1.7.1",
    "socket.io-browserify": "~0.9.6",
    "socket.io-client": "^1.7.1",
    "socket.io-redis": "^2.0.0",
    "stylus": "~0.32.1",
    "sugar": "~1.3.8",
    "superagent": "^0.15.7",
    "unzip": "~0.1.7",
    "validator": "^3.5.1"
  },
  "engines": {
    "node": "^0.12.0",
    "npm": "^2.0.0"
  },
  "devDependencies": {
    "autoless": "^0.1.7",
    "bluebird": "^2.7.1",
    "bower": "^1.4.1",
    "browserify": "~2.35.1",
    "bunyan": "^1.4.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.0.0",
    "css-loader": "^0.14.4",
    "docker-hub-api": "^0.5.1",
    "dotenv": "~2.0.0",
    "dust-loader": "^1.0.1",
    "exports-loader": "^0.6.2",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "mocha": "^3.2.0",
    "parallelshell": "^1.1.1",
    "proxyquire": "~0.5.0",
    "sandboxed-module": "^2.0.3",
    "sinon": "^1.17.4",
    "sinon-as-promised": "^3.0.0",
    "sinon-chai": "^2.5.0",
    "style-loader": "^0.12.3",
    "supertest": "~0.8.0",
    "uglify-js": "^2.3.6",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.9.0"
  },
  "asq": {
    "corePlugins": [
      "asq-buckets-q",
      "asq-canvas",
      "asq-code-q",
      "asq-css-select-q",
      "asq-exercise",
      "asq-fiddle-q",
      "asq-highlight-q",
      "asq-js-eval-q",
      "asq-js-function-body-q",
      "asq-multi-choice-q",
      "asq-order-q",
      "asq-rating-q",
      "asq-settings",
      "asq-sqlite-q",
      "asq-text-input-q",
      "asq-live-app"
    ]
  },
  "keywords": [
    "node.js"
  ],
  "license": "MIT",
  "readmeFilename": "README.md"
}
