{>"questionForm-viewer"/}

{<questionContent}
  <ol>
  {#question.questionOptions formButtonType=question.formButtonType htmlId=question.htmlId}
    <li class="{classList}" >
      <label class="{formButtonType}">
        <input type="{formButtonType}" name="{htmlId}" value="{$idx}"> {text|s}
      </label>
    </li>{~n}
  {/question.questionOptions}
</ol>
{/questionContent}